[
  {
    "alertMessage": "Pod: nginx has containers that do not set 'securityContext.procMount' to 'Default'",
    "packagename": "armo_builtins",
    "alertScore": 7,
    "fixPaths": [
      {
        "path": "spec.containers[1].securityContext.procMount",
        "value": "Default"
      }
    ],
    "ruleStatus": "",
    "alertObject": {
      "k8sApiObjects": [
        {
          "apiVersion": "v1",
          "kind": "Pod",
          "metadata": {
            "name": "nginx"
          }
        }
      ]
    }
  }
]